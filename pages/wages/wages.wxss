/**工资**/
page {
  height: 100%;
  background: #eeeeee;
}
.section-title {
  height: 100rpx;
  line-height: 100rpx;
  color: #333333;
  padding-left: 10px;
}
.my-item {
  width: 100%;
  border-bottom: 1px solid #eee;
  height: 100rpx;
  line-height: 100rpx;
  position: relative;
  background: #ffffff;
  padding-left: 10px;
}
.table-list {
  background: #ffffff;
  width: 100%;
  overflow: auto;
}
.table-title {
  color: #333333;
  font-weight: bold;
}
.table-row {
  border-bottom: 1px solid #eeeeee;
  display:flex;
  justify-content: left;
  align-items: center;
  white-space: nowrap;
}
.table-th {
  width: 100%;
}
.table-th .table-cell,.table-th .table-num {
  background: #EE2B7A;
  color: #ffffff;
}
.table-tr {
  width: 100%;
}
.table-cell {
  border-right: 1px solid #eeeeee;
  padding: 2px;
  min-width: 200px;
  height:30px;
  line-height: 30px;
}
.table-cell .table-num  {
  width: 60px;
  max-width:60px;
}
.primary-btn {
  margin: 40rpx;  
}
.table-even .table-cell,.table-even .table-num{
  background: #E6F3F9;
}